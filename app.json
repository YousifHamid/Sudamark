{
  "expo": {
    "name": "Sudamark",
    "slug": "sudamark",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sudamark",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sudamark.app",
      "infoPlist": {
        "NSCameraUsageDescription": "SudaMark needs camera access to take photos of your car listings",
        "NSPhotoLibraryUsageDescription": "SudaMark needs photo library access to upload car photos",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "whatsapp",
          "tel",
          "mailto"
        ]
      },
      "buildNumber": "2"
    },
    "android": {
      "package": "com.sudamark.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "versionCode": 2
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111827",
          "dark": {
            "backgroundColor": "#111827"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "a72ceeb5-4da9-4d16-8d62-48704991a62e"
      }
    }
  }
}